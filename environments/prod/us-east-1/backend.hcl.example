# Backend config example: copy to backend.hcl and fill values
# S3 bucket name must be globally unique: terraform-state-{ACCOUNT_ID}-{REGION}
# Get Account ID: aws sts get-caller-identity --query Account --output text

bucket         = "terraform-state-ACCOUNT_ID-us-east-1"  # Replace ACCOUNT_ID
key            = "prod/us-east-1/terraform.tfstate"
region         = "us-east-1"
dynamodb_table = "terraform-state-lock"
encrypt        = true
# kms_key_id     = "arn:aws:kms:us-east-1:ACCOUNT_ID:key/KEY_ID" # Optional

